{
  "title": "Launch websites with hyper + letters",
  "rules": [
    {
      "description": "Launch websites with hyper + letters",
      "manipulators": [
        {
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": [
                "left_shift",
                "left_control",
                "left_option",
                "left_command"
              ]
            }
          },
          "to": [{ "shell_command": "open 'https://mail.google.com'" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "g",
            "modifiers": {
              "mandatory": [
                "left_shift",
                "left_control",
                "left_option",
                "left_command"
              ]
            }
          },
          "to": [
            { "shell_command": "open 'https://github.com/discord/discord'" }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "left_shift",
                "left_control",
                "left_option",
                "left_command"
              ]
            }
          },
          "to": [{ "shell_command": "open 'https://calendar.google.com'" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "r",
            "modifiers": {
              "mandatory": [
                "left_shift",
                "left_control",
                "left_option",
                "left_command"
              ]
            }
          },
          "to": [{ "shell_command": "open 'https://github.com/pulls'" }],
          "type": "basic"
        }
      ]
    }
  ]
}
