{
  "editor.fontFamily": "MesloLGS Nerd Font",
  "editor.cursorSurroundingLines": 5,
  "editor.formatOnSaveMode": "file",
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.rulers": [
    120
  ],
  "editor.tabCompletion": "on",
  "editor.trimAutoWhitespace": true,
  "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.branchPrefix": "pcho90/",
  "python.languageServer": "Default",
  "security.workspace.trust.untrustedFiles": "open",
  "window.commandCenter": true,
  "workbench.fontAliasing": "auto",
  "workbench.layoutControl.type": "menu",
  "workbench.settings.editor": "json",
  "zenMode.fullScreen": false,
  "zenMode.hideLineNumbers": false,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.leader": " ",
  "vim.useCtrlKeys": false,
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "vim.handleKeys": {
    "<C-[>": true,
    "<C-a>": true,
    "<C-d>": true,
    "<C-e>": true,
    "<C-i>": true,
    "<C-m>": true,
    "<C-o>": true,
    "<C-r>": true,
    "<C-t>": true,
    "<C-u>": true,
    "<C-v>": true,
    "<C-w>": true,
    "<C-x>": true,
    "<C-y>": true,
    "<C-j>": true,
    "<C-k>": true,
    "<C-h>": true,
    "<C-l>": true
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.showDefinitionPreviewHover"
      ]
    },
    {
      "before": [
        "<CR>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "g",
        "D"
      ],
      "commands": [
        "editor.action.peekDefinition"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "editor.action.referenceSearch.trigger"
      ]
    },
    {
      "before": [
        "<space>",
        "t",
        "o"
      ],
      "commands": [
        ":tabo"
      ]
    },
    {
      "before": [
        "<leader>",
        "w",
        "j"
      ],
      "commands": [
        "workbench.action.moveEditorToBelowGroup"
      ]
    },
    {
      "before": [
        "<leader>",
        "w",
        "k"
      ],
      "commands": [
        "workbench.action.moveEditorToAboveGroup"
      ]
    },
    {
      "before": [
        "<leader>",
        "w",
        "h"
      ],
      "commands": [
        "workbench.action.moveEditorToLeftGroup"
      ]
    },
    {
      "before": [
        "<leader>",
        "w",
        "l"
      ],
      "commands": [
        "workbench.action.moveEditorToRightGroup"
      ]
    },
    {
      "before": [
        "<space>",
        "s",
        "e"
      ],
      "commands": [
        "search.action.openEditor"
      ]
    },
    {
      "before": [
        "<space>",
        "s",
        "v"
      ],
      "commands": [
        "search.action.openNewEditorToSide"
      ]
    },
    {
      "before": [
        "<space>",
        "t",
        "h"
      ],
      "commands": [
        "workbench.action.closeEditorsToTheLeft"
      ]
    },
    {
      "before": [
        "<space>",
        "t",
        "l"
      ],
      "commands": [
        "workbench.action.closeEditorsToTheRight"
      ]
    },
    {
      "before": [
        "<space>",
        "f",
        "d"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "<space>",
        "f",
        "s"
      ],
      "commands": [
        "editor.action.formatSelection"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "h"
      ],
      "commands": [
        "gitlens.showQuickFileHistory"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "c"
      ],
      "commands": [
        "git.openChange"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "a"
      ],
      "commands": [
        "git.openAllChanges"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "s"
      ],
      "commands": [
        "workbench.view.scm"
      ]
    },
    {
      "before": [
        "<space>",
        "w",
        "o"
      ],
      "commands": [
        "workbench.action.focusOtherSideEditor",
        {
          "command": "cursorMove",
          "args": {
            "to": "viewPortCenter"
          }
        }
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        "v"
      ],
      "commands": [
        "bookmarksExplorer.focus"
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        "m"
      ],
      "commands": [
        "bookmarks.toggle"
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        "l"
      ],
      "commands": [
        "bookmarks.list"
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        "a"
      ],
      "commands": [
        "bookmarks.listFromAllFiles"
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        "c"
      ],
      "commands": [
        "bookmarks.clear"
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        "r"
      ],
      "commands": [
        "bookmarks.clearFromAllFiles"
      ]
    },
    {
      "before": [
        "<space>",
        "n",
        "c"
      ],
      "commands": [
        "notifications.clearAll"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "i"
      ],
      "commands": [
        "merge-conflict.accept.incoming"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "o"
      ],
      "commands": [
        "merge-conflict.accept.current"
      ]
    },
    {
      "before": [
        "<space>",
        "p",
        "r"
      ],
      "commands": [
        "pr:github.focus"
      ]
    },
    {
      "before": [
        "[",
        "m"
      ],
      "commands": [
        "bookmarks.jumpToPrevious"
      ]
    },
    {
      "before": [
        "[",
        "g"
      ],
      "commands": [
        "gitlens.diffWithPreviousInDiffRight"
      ]
    },
    {
      "before": [
        "]",
        "g"
      ],
      "commands": [
        "gitlens.diffWithNextInDiffRight"
      ]
    },
    {
      "before": [
        "[",
        "G"
      ],
      "commands": [
        "workbench.action.compareEditor.previousChange"
      ]
    },
    {
      "before": [
        "]",
        "G"
      ],
      "commands": [
        "workbench.action.compareEditor.nextChange"
      ]
    },
    {
      "before": [
        "]",
        "m"
      ],
      "commands": [
        "bookmarks.jumpToNext"
      ]
    },
    {
      "before": [
        "[",
        "D"
      ],
      "commands": [
        "editor.action.marker.prevInFiles"
      ]
    },
    {
      "before": [
        "]",
        "D"
      ],
      "commands": [
        "editor.action.marker.nextInFiles"
      ]
    },
    {
      "before": [
        "[",
        "d"
      ],
      "commands": [
        "editor.action.marker.prev"
      ]
    },
    {
      "before": [
        "]",
        "d"
      ],
      "commands": [
        "editor.action.marker.next"
      ]
    },
    {
      "before": [
        "[",
        "C"
      ],
      "commands": [
        "editor.action.dirtydiff.previous"
      ]
    },
    {
      "before": [
        "]",
        "C"
      ],
      "commands": [
        "editor.action.dirtydiff.next"
      ]
    },
    {
      "before": [
        "[",
        "c"
      ],
      "commands": [
        "workbench.action.editor.previousChange"
      ]
    },
    {
      "before": [
        "]",
        "c"
      ],
      "commands": [
        "workbench.action.editor.nextChange"
      ]
    },
    {
      "before": [
        "[",
        "h"
      ],
      "commands": [
        "workbench.action.compareEditor.previousChange"
      ]
    },
    {
      "before": [
        "]",
        "h"
      ],
      "commands": [
        "workbench.action.compareEditor.nextChange"
      ]
    }
  ],
  "vim.operatorPendingModeKeyBindings": [
    {
      "before": [
        "L"
      ],
      "after": [
        "$"
      ]
    },
    {
      "before": [
        "H"
      ],
      "after": [
        "^"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "p"
      ],
      "after": [
        "p",
        "g",
        "v",
        "y"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "r"
      ],
      "commands": [
        "git.revertSelectedRanges"
      ]
    }
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "scm.diffDecorationsGutterPattern": {
    "modified": false
  },
  "go.toolsManagement.autoUpdate": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
}
