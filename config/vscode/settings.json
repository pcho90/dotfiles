{
  "editor.fontFamily": "MesloLGS NF",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "workbench.iconTheme": "material-icon-theme",
  "editor.trimAutoWhitespace": true,
  "editor.formatOnSaveMode": "file",
  "editor.tabCompletion": "on",
  "editor.lineNumbers": "relative",
  "editor.cursorSurroundingLines": 8,
  "editor.rulers": [
    120
  ],
  "editor.minimap.enabled": false,
  "files.autoSave": "afterDelay",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.eol": "\n",
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "git.autofetch": true,
  "material-icon-theme.hidesExplorerArrows": true,
  "python.languageServer": "Default",
  "workbench.layoutControl.type": "menu",
  "workbench.settings.editor": "json",
  "security.workspace.trust.untrustedFiles": "open",
  "zenMode.fullScreen": false,
  "zenMode.hideLineNumbers": false,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "vim.leader": " ",
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.useCtrlKeys": false,
  "vim.visualstar": true,
  "vim.handleKeys": {
    "<C-[>": true,
    "<C-a>": true,
    "<C-d>": true,
    "<C-e>": true,
    "<C-i>": true,
    "<C-m>": true,
    "<C-o>": true,
    "<C-r>": true,
    "<C-t>": true,
    "<C-u>": true,
    "<C-v>": true,
    "<C-w>": true,
    "<C-x>": true,
    "<C-y>": true,
    "<C-j>": true,
    "<C-k>": true,
    "<C-h>": true,
    "<C-l>": true
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "g",
        "w"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "<leader>",
        "b",
        "d",
        "w"
      ]
    },
    {
      "before": [
        "g",
        "D"
      ],
      "commands": [
        "editor.action.peekDefinition"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "editor.action.referenceSearch.trigger"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.showDefinitionPreviewHover"
      ]
    },
    {
      "before": [
        "<CR>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "<space>",
        "t",
        "o"
      ],
      "commands": [
        ":tabo"
      ]
    },
    {
      "before": [
        "<leader>",
        "w",
        "j"
      ],
      "commands": [
        "workbench.action.moveEditorToBelowGroup"
      ]
    },
    {
      "before": [
        "<leader>",
        "w",
        "k"
      ],
      "commands": [
        "workbench.action.moveEditorToAboveGroup"
      ]
    },
    {
      "before": [
        "<leader>",
        "w",
        "h"
      ],
      "commands": [
        "workbench.action.moveEditorToLeftGroup"
      ]
    },
    {
      "before": [
        "<leader>",
        "w",
        "l"
      ],
      "commands": [
        "workbench.action.moveEditorToRightGroup"
      ]
    },
    {
      "before": [
        "<space>",
        "s",
        "e"
      ],
      "commands": [
        "search.action.openEditor"
      ]
    },
    {
      "before": [
        "<space>",
        "s",
        "v"
      ],
      "commands": [
        "search.action.openNewEditorToSide"
      ]
    },
    {
      "before": [
        "<space>",
        "t",
        "h"
      ],
      "commands": [
        "workbench.action.closeEditorsToTheLeft"
      ]
    },
    {
      "before": [
        "<space>",
        "t",
        "l"
      ],
      "commands": [
        "workbench.action.closeEditorsToTheRight"
      ]
    },
    {
      "before": [
        "<space>",
        "f",
        "d"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "<space>",
        "f",
        "s"
      ],
      "commands": [
        "editor.action.formatSelection"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "h"
      ],
      "commands": [
        "gitlens.showQuickFileHistory"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "c"
      ],
      "commands": [
        "git.openChange"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "a"
      ],
      "commands": [
        "git.openAllChanges"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "s"
      ],
      "commands": [
        "workbench.view.scm"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "e"
      ],
      "commands": [
        "workbench.action.focusOtherSideEditor",
        {
          "command": "cursorMove",
          "args": {
            "to": "viewPortCenter"
          }
        }
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        "v"
      ],
      "commands": [
        "bookmarksExplorer.focus"
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        "m"
      ],
      "commands": [
        "bookmarks.toggle"
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        "l"
      ],
      "commands": [
        "bookmarks.list"
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        "a"
      ],
      "commands": [
        "bookmarks.listFromAllFiles"
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        "c"
      ],
      "commands": [
        "bookmarks.clear"
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        "r"
      ],
      "commands": [
        "bookmarks.clearFromAllFiles"
      ]
    },
    {
      "before": [
        "<space>",
        "n",
        "c"
      ],
      "commands": [
        "notifications.clearAll"
      ]
    },
    {
      "before": [
        "[",
        "m"
      ],
      "commands": [
        "bookmarks.jumpToPrevious"
      ]
    },
    {
      "before": [
        "[",
        "g"
      ],
      "commands": [
        "gitlens.diffWithPreviousInDiffRight"
      ]
    },
    {
      "before": [
        "]",
        "g"
      ],
      "commands": [
        "gitlens.diffWithNextInDiffRight"
      ]
    },
    {
      "before": [
        "]",
        "m"
      ],
      "commands": [
        "bookmarks.jumpToNext"
      ]
    },
    {
      "before": [
        "[",
        "D"
      ],
      "commands": [
        "editor.action.marker.prevInFiles"
      ]
    },
    {
      "before": [
        "]",
        "D"
      ],
      "commands": [
        "editor.action.marker.nextInFiles"
      ]
    },
    {
      "before": [
        "[",
        "d"
      ],
      "commands": [
        "editor.action.marker.prev"
      ]
    },
    {
      "before": [
        "]",
        "d"
      ],
      "commands": [
        "editor.action.marker.next"
      ]
    },
    {
      "before": [
        "[",
        "C"
      ],
      "commands": [
        "editor.action.dirtydiff.next"
      ]
    },
    {
      "before": [
        "]",
        "C"
      ],
      "commands": [
        "editor.action.dirtydiff.previous"
      ]
    },
    {
      "before": [
        "[",
        "c"
      ],
      "commands": [
        "workbench.action.editor.previousChange"
      ]
    },
    {
      "before": [
        "]",
        "c"
      ],
      "commands": [
        "workbench.action.editor.nextChange"
      ]
    },
    {
      "before": [
        "[",
        "h"
      ],
      "commands": [
        "workbench.action.compareEditor.previousChange"
      ]
    },
    {
      "before": [
        "]",
        "h"
      ],
      "commands": [
        "workbench.action.compareEditor.nextChange"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "i"
      ],
      "commands": [
        "merge-conflict.accept.incoming"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "o"
      ],
      "commands": [
        "merge-conflict.accept.current"
      ]
    },
    {
      "before": [
        "<space>",
        "p",
        "r"
      ],
      "commands": [
        "pr:github.focus"
      ]
    }
  ],
  "vim.operatorPendingModeKeyBindings": [
    {
      "before": [
        "L"
      ],
      "after": [
        "$"
      ]
    },
    {
      "before": [
        "H"
      ],
      "after": [
        "^"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "p"
      ],
      "after": [
        "p",
        "g",
        "v",
        "y"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "r"
      ],
      "commands": [
        "git.revertSelectedRanges"
      ]
    }
  ],
  "remote.SSH.defaultForwardedPorts": [
    {
      "name": "supervisord",
      "localPort": 9001,
      "remotePort": 9000
    },
    {
      "name": "react_native_metro",
      "localPort": 8081,
      "remotePort": 8081
    },
    {
      "name": "discord_marketing",
      "localPort": 3334,
      "remotePort": 3334
    },
    {
      "name": "discord_web",
      "localPort": 3333,
      "remotePort": 3333
    },
    {
      "name": "discord_developers",
      "localPort": 3336,
      "remotePort": 3336
    },
    {
      "name": "elasticsearch",
      "localPort": 9200,
      "remotePort": 9200
    },
    {
      "name": "elasticsearch",
      "localPort": 9300,
      "remotePort": 9300
    },
    {
      "name": "discord_remote_auth_gateway",
      "localPort": 3101,
      "remotePort": 3101
    },
    {
      "name": "discord_api",
      "localPort": 3000,
      "remotePort": 3000
    },
    {
      "name": "discord_uikit",
      "localPort": 6006,
      "remotePort": 6006
    },
    {
      "name": "smite_ui_api",
      "localPort": 5004,
      "remotePort": 5004
    },
    {
      "name": "discord_admin_web",
      "localPort": 6001,
      "remotePort": 6001
    },
    {
      "name": "discord_gateway",
      "localPort": 3004,
      "remotePort": 3004
    },
    {
      "name": "discord_voice",
      "localPort": 3002,
      "remotePort": 3002
    },
    {
      "name": "discord_admin",
      "localPort": 6008,
      "remotePort": 6008
    }
  ]
}
